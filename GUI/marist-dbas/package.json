{
  "name": "marist-dbas",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.2",
    "concurrently": "^7.5.0",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "jquery": "^3.6.1",
    "jquery-validation": "^1.19.5",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "sequelize": "^6.25.3",
    "util": "^0.12.5",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "nodemon --watch ./public/* --exec \"electron .\"",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && npm run start-electron\" ",
    "electron": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "crypto": false
  },
  "devDependencies": {
    "@types/node": "^18.11.9"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "plugins": [
        "@electron-forge/plugin-webpack",
        {
          "renderer": {
            "entryPoints": [
              {
                "js": "./src/electron/renderer.js"
              }
            ]
          }
        }
      ]
    }
  }
}
